{
  "name": "xparse/parser",
  "description": "Parser client",
  "keywords": [
    "grabber",
    "parser"
  ],
  "homepage": "https://github.com/xparse/Parser",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Shcherbak",
      "email": "alotofall@gmail.com",
      "homepage": "http://github.com/funivan/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "^6.3",
    "xparse/element-finder": "^0.3.1"
  },
  "minimum-stability": "alpha",
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Xparse\\Parser\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Xparse\\Parser\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.1"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
